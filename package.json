{
  "name": "capacitor-naver-login",
  "version": "0.0.1",
  "description": "Naver login plugin",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Heo Sangmin",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^3.0.0-beta.1",
    "@capacitor/core": "^3.0.0-beta.1",
    "@capacitor/ios": "^3.0.0-beta.1",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-java": "^1.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "swiftlint": "^1.0.1",
    "typescript": "~4.1.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorNaverLogin.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/leo6104/capacitor-naver-login.git"
  },
  "bugs": {
    "url": "https://github.com/leo6104/capacitor-naver-login/issues"
  },
  "dependencies": {}
}
